{
  "name": "ln-client-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "npm run build && npm run start:server",
    "start:server": "node dist/app",
    "prettier": "prettier --single-quote --print-width 100 --trailing-comma es5 --write 'src/**/*.ts'",
    "gen-ts-proto": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:generated --ts_out=service=true:generated -I ./src/proto src/proto/*.proto"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.36",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/helmet": "^0.0.37",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.0",
    "@types/qrcode": "^0.8.0",
    "@types/raven": "^2.1.5",
    "@types/winston": "^2.3.7",
    "@types/ws": "^4.0.0",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.2",
    "ts-protoc-gen": "^0.4.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/uuid": "^3.4.3",
    "aws-sdk": "^2.188.0",
    "bignumber.js": "^6.0.0",
    "bytebuffer": "^5.0.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-ws": "^3.0.0",
    "google-protobuf": "^3.5.0",
    "grpc": "^1.8.4",
    "grpc-caller": "^0.4.0",
    "helmet": "^3.10.0",
    "morgan": "^1.9.0",
    "protobufjs": "^6.8.4",
    "qrcode": "^1.2.0",
    "raven": "^2.4.1",
    "uuid": "^3.2.1",
    "winston": "^2.4.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
